{
  "name": "fk-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "rn-nodeify --install stream,process,util --hack",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "browser": {
    "net": "react-native-tcp",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "es6-symbol": "3.1.1",
    "fk-app-protocol": "file:../app-protocol",
    "fk-atlas-protocol": "file:../atlas/protocol",
    "fk-data-protocol": "file:../data-protocol",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "process": "0.11.10",
    "protobufjs": "6.8.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.47.1",
    "react-native-animated-bar": "^0.2.0",
    "react-native-fs": "2.8.1",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-native-mail": "3.0.4",
    "react-native-progress-circle": "2.0.0",
    "react-native-service-discovery": "https://github.com/Conservify/react-native-service-discovery.git",
    "react-native-simple-toast": "0.0.8",
    "react-native-svg": "5.4.2",
    "react-native-tcp": "https://github.com/PeelTechnologies/react-native-tcp.git",
    "react-native-udp": "^2.3.1",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.6",
    "readable-stream": "1.0.33",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.6",
    "redux-thunk": "2.2.0",
    "rxjs": "5.4.3",
    "stream-browserify": "1.0.0",
    "varint": "^5.0.0",
    "victory-native": "0.15.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.0.0",
    "jest": "20.0.4",
    "jest-html-reporter": "0.5.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-saga-tester": "1.0.373",
    "rn-nodeify": "7.0.1"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "__ENV__": "test",
      "__SPECS__": true
    },
    "setupFiles": [
      "./specs/setup.js"
    ],
    "testMatch": [
      "<rootDir>/app/**/(.*)specs.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "react-native": {
    "net": "react-native-tcp",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
