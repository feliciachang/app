    "fk-data-protocol": "https://github.com/fieldkit/data-protocol.git",
    "i18n-js": "^3.0.11",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "process": "0.11.10",
    "protobufjs": "^6.8.8",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
